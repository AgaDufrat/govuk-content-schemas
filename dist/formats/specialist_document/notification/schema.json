      "properties": {
        "path": {
          "type": "string"
        },
        "type": {
          "enum": [
            "prefix",
            "exact"
          ]
        }
      },
      "required": [
        "path",
        "type"
      ],
      "type": "object"
    },
    "withdrawn_notice": {
