        "smartanswers",
        "specialist-publisher",
        "spotlight",
        "static",
        "tariff",
        "travel-advice-publisher",
        "whitehall"
      ],
      "type": "string"
    },
    "rendering_app_name": {
      "description": "The application that renders this item.",
      "enum": [
        "calculators",
        "calendars",
        "collections",
        "designprinciples",
        "email-alert-frontend",
        "email-campaign-frontend",
        "feedback",
