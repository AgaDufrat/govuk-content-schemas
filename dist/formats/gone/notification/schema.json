{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "content_id",
    "links"
  ],
  "properties": {
    "base_path": {
      "$ref": "#/definitions/absolute_path"
    },
    "content_id": {
      "$ref": "#/definitions/guid"
    },
    "document_type": {
      "enum": [
        "gone"
      ]
    },
    "schema_name": {
      "enum": [
        "gone"
      ]
    },
    "publishing_app": {
      "type": "string"
    },
    "public_updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "details": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "explanation": {
          "type": [
            "string",
            "null"
          ]
        },
        "alternative_path": {
          "type": [
            "string",
            "null"
          ],
          "format": "uri"
        }
      }
    },
    "links": {
      "additionalProperties": false,
      "type": "object",
      "properties": {
        "available_translations": {
          "description": "An automatic link added by the Publising API",
          "$ref": "#/definitions/expanded_links"
        },
        "children": {
          "description": "An automatic link added by the Publising API",
          "$ref": "#/definitions/expanded_links"
        },
        "policies": {
          "description": "An automatic link added by the Publising API",
          "$ref": "#/definitions/expanded_links"
        },
        "document_collections": {
          "description": "An automatic link added by the Publising API",
          "$ref": "#/definitions/expanded_links"
        },
        "child_taxons": {
          "description": "An automatic link added by the Publising API",
          "$ref": "#/definitions/expanded_links"
        },
        "topic_content": {
          "description": "An automatic link added by the Publising API",
          "$ref": "#/definitions/expanded_links"
        },
        "mainstream_browse_content": {
          "description": "An automatic link added by the Publising API",
          "$ref": "#/definitions/expanded_links"
        }
      }
    },
    "format": {
      "type": "string",
      "description": "DEPRECATED: use `document_type` instead. This field will be removed."
    },
    "navigation_document_supertype": {
      "type": "string",
      "description": "Document type grouping powering the new taxonomy-based navigation pages"
    },
    "user_journey_document_supertype": {
      "type": "string",
      "description": "Document type grouping powering analytics of user journeys"
    },
    "whitehall_document_supertype": {
      "type": "string",
      "description": "Document type grouping intended to power the Whitehall finders and email subscriptions"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "definitions": {
    "expanded_links": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "content_id",
          "title",
          "base_path",
          "locale"
        ],
        "properties": {
          "details": {
            "type": "object",
            "additionalProperties": true
          },
          "document_type": {
            "type": "string"
          },
          "schema_name": {
            "type": "string"
          },
          "public_updated_at": {
            "type": "string"
          },
          "content_id": {
            "$ref": "#/definitions/guid"
          },
          "title": {
            "type": "string"
          },
          "description": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ]
          },
          "base_path": {
            "$ref": "#/definitions/absolute_path"
          },
          "api_path": {
            "$ref": "#/definitions/absolute_path"
          },
          "locale": {
            "$ref": "#/definitions/locale"
          },
          "analytics_identifier": {
            "$ref": "#/definitions/analytics_identifier"
          },
          "withdrawn": {
            "description": "Indicates whether the item has been unpublished as withdrawn, https://github.com/alphagov/publishing-api/blob/master/doc/model.md#workflow",
            "type": "boolean"
          },
          "country": {
            "$ref": "#/definitions/country"
          },
          "change_description": {
            "type": "string"
          },
          "links": {
            "type": "object",
            "patternProperties": {
              "^[a-z_]+$": {
                "$ref": "#/definitions/expanded_links"
              }
            }
          }
        }
      }
    },
    "absolute_path": {
      "type": "string",
      "pattern": "^/(([a-zA-Z0-9._~!$&'()*+,;=:@-]|%[0-9a-fA-F]{2})+(/([a-zA-Z0-9._~!$&'()*+,;=:@-]|%[0-9a-fA-F]{2})*)*)?$"
    },
    "guid": {
      "type": "string",
      "pattern": "^[a-f0-9]{8}-[a-f0-9]{4}-[1-5][a-f0-9]{3}-[89ab][a-f0-9]{3}-[a-f0-9]{12}$"
    },
    "guid_list": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/guid"
      }
    }
  }
}
